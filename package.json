{
  "name": "@blurname/blurkit",
  "version": "0.0.27",
  "author": "blurname <naughtybao@outlook.com>",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@7.25.0",
  "scripts": {
    "version-bump": "tsx ./scripts/version-bump.ts",
    "tag-push": "pnpm build && changeset publish",
    "release": "pnpm version-bump && pnpm tag-push",
    "dev:svgminify": "pnpm --filter @blurkit/svgminify dev",
    "build": "pnpm -r build",
    "typecheck": "tsc --noEmit",
    "clean:dist": "rm -rf dist packages/{core,cli,web,svgminify}/dist",
    "clean:node_modules": "rm -rf node_modules packages/{core,cli,web}/node_modules"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@dr-js/dev": "^0.5.7",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-react": "^3.1.0",
    "esbuild": "^0.17.16",
    "eslint": "^8.38.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lit": "^1.8.2",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "pnpm": "^8.2.0",
    "tsx": "^3.12.6",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.1"
  }
}