{
  "name": "@blurname/blurkit",
  "version": "0.0.27",
  "author": "blurname <naughtybao@outlook.com>",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@7.25.0",
  "scripts": {
    "version-bump": "tsx ./scripts/monorepo.ts version-bump",
    "tag-push": "pnpm build && changeset publish",
    "release": "pnpm version-bump && pnpm tag-push",
    "build": "pnpm -r build",
    "typecheck": "tsc --noEmit",
    "dev:svgminify": "pnpm --filter @blurkit/svgminify dev",
    "dev:web": "pnpm --filter @blurkit/web dev",
    "clean:dist": "tsx ./scripts/monorepo.ts clean-dist",
    "clean:node_modules": "tsx ./scripts/monorepo.ts clean-node-modules"
  },
  "devDependencies": {
    "@dr-js/dev": "^0.5.9",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^4.0.0",
    "esbuild": "^0.18.2",
    "eslint": "^8.42.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lit": "^1.8.3",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "pnpm": "^8.6.2",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  }
}
