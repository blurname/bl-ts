{
  "name": "@blurname/blurkit",
  "version": "0.0.41",
  "author": "blurname <naughtybao@outlook.com>",
  "type": "module",
  "private": false,
  "scripts": {
    "version-bump": "tsx ./scripts/monorepo.ts version-bump",
    "tag-push": "pnpm build && pnpm -r publish",
    "release": "pnpm version-bump && pnpm tag-push",
    "build": "pnpm -r build",
    "typecheck": "tsc --noEmit",
    "dev-svgminify": "pnpm --filter @blurkit/svgminify dev",
    "dev-lost": "pnpm --filter @blurkit/lost dev",
    "clean-dist": "tsx ./scripts/monorepo.ts clean-dist",
    "clean-node_modules": "tsx ./scripts/monorepo.ts clean-node-modules",
    "clean-lock": "tsx ./scripts/monorepo.ts clean-lock",
    "test-command": "echo test"
  },
  "devDependencies": {
    "@dr-js/dev": "^0.5.9",
    "@types/node": "^20.3.3",
    "@types/react": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react": "^4.0.1",
    "esbuild": "^0.18.11",
    "eslint": "^8.44.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-lit": "^1.8.3",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "tsx": "^3.13.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9"
  }
}