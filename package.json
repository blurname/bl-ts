{
  "name": "@blurname/blurkit",
  "version": "0.0.13",
  "author": "blurname <naughtybao@outlook.com>",
  "type": "module",
  "private": false,
  "packageManager": "pnpm@7.25.0",
  "scripts": {
    "version-bump": "tsx ./scripts/version-bump.ts",
    "tag-push":"pnpm build && changeset publish",
    "release": "pnpm version-bump && pnpm tag-push",
    "dev": "vite",
    "build": "pnpm -r build",
    "buildv": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean:dist": "rm -rf dist packages/{core,cli,web}/dist",
    "clean:node_modules": "rm -rf node_modules packages/{core,cli,web}/node_modules"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.53",
    "@vitejs/plugin-react": "^1.3.2",
    "esbuild": "^0.14.54",
    "pnpm": "^7.26.2",
    "prettier": "^2.8.3",
    "prop-types": "^15.8.1",
    "rimraf": "^3.0.2",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vitest": "^0.23.4"
  }
}
